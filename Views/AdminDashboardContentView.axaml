<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:pr.ViewModels"
             mc:Ignorable="d" d:DesignWidth="600" d:DesignHeight="400"
             x:Class="pr.Views.AdminDashboardContentView"
             x:DataType="vm:AdminDashboardContentViewModel"
             Background="White">
  <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
    <TextBlock Text="{Binding DashboardTitle}" FontSize="24" HorizontalAlignment="Center" Foreground="Black" />
    <TextBlock Text="{Binding WelcomeText}" HorizontalAlignment="Center" Margin="0,20,0,0" Foreground="Black" />
  </StackPanel>
</UserControl>
